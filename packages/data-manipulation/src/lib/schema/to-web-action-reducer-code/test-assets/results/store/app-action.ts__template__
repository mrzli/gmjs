import { ActionGeneric, ActionTypeGeneric } from './generic-action';
import { ActionEntity, ActionTypeEntity } from './entity/action';
import { ActionEntity2, ActionTypeEntity2 } from './entity2/action';
import {
  ActionAnotherEntity,
  ActionTypeAnotherEntity,
} from './another-entity/action';

export type AppActionType =
  | ActionTypeGeneric
  | ActionTypeEntity
  | ActionTypeEntity2
  | ActionTypeAnotherEntity;

export type AppAction =
  | ActionGeneric
  | ActionEntity
  | ActionEntity2
  | ActionAnotherEntity;
